{
  "name": "status-jockey",
  "version": "1.1.1",
  "description": "Response filter for Statuspage.io API",
  "main": "./dist/index.js",
  "scripts": {
    "test": "nyc --reporter=lcov babel-node node_modules/.bin/jasmine JASMINE_CONFIG_PATH=./src/spec/support/jasmine.json && yarn coveralls",
    "test:debugger": "babel-node --inspect-brk node_modules/.bin/jasmine JASMINE_CONFIG_PATH=./src/spec/support/jasmine.json",
    "coveralls": "yarn istanbul-coveralls",
    "babelify": "babel src --out-dir dist --ignore spec/",
    "build": "BABEL_ENV=production yarn babelify",
    "prepare": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NYULibraries/statusjockey.git"
  },
  "author": "Elliott Grieco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NYULibraries/statusjockey/issues"
  },
  "homepage": "https://github.com/NYULibraries/statusjockey",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "istanbul-coveralls": "^1.0.3",
    "jasmine": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "nock": "^9.2.5",
    "nyc": "^11.7.1",
    "rewire": "^4.0.1"
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "nyc": {
    "exclude": [
      "src/spec/**/*"
    ]
  }
}
