{
  "name": "status-jockey",
  "version": "1.0.0",
  "description": "Response filter for Statuspage.io API",
  "main": "./dist/index.js",
  "scripts": {
    "test": "babel-node node_modules/.bin/jasmine JASMINE_CONFIG_PATH=./src/spec/support/jasmine.json",
    "coverage": "istanbul cover jasmine JASMINE_CONFIG_PATH=./src/spec/support/jasmine.json",
    "babelify": "babel src --out-dir dist --ignore *.spec.js",
    "build": "BABEL_ENV=production yarn babelify",
    "prepare": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NYULibraries/statusjockey.git"
  },
  "author": "Elliott Grieco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NYULibraries/statusjockey/issues"
  },
  "homepage": "https://github.com/NYULibraries/statusjockey",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "istanbul": "^0.4.5",
    "jasmine": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1"
  }
}
